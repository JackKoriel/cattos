{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "dev": {
      "cache": false,
      "persistent": true,
      "dependsOn": ["^dev"],
      "outputs": []
    },
    "start": {
      "cache": false,
      "persistent": true,
      "outputs": [],
      "dependsOn": ["build"]
    },
    "build": {
      "outputs": ["dist/**", "build/**", ".next/**"],
      "dependsOn": ["^build"]
    },
    "lint": {
      "outputs": [],
      "cache": true
    }
  }
}
